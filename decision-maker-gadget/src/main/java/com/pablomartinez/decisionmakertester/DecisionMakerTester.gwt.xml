<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='decisionMakerTester'>
  <inherits name='cc.kune.gadget.KuneGadget'/>
  <inherits name='com.pablomartinez.decisionmaker.DecisionMaker'/>

  <!-- Other module inherits -->
  <inherits name='com.thezukunft.wave.WaveConnectorMock'/>

  <!-- Specify the gadget tester entry point class. Run this as a GWT Web 
    app to test it in development mode -->
  <entry-point
    class='com.pablomartinez.decisionmakertester.client.DecisionMakerTesterEntryPoint'/>

  <!-- Specify the paths for translatable code -->
  <source path='client'/>
  
  <set-property name="user.agent" value="safari"></set-property>

</module>
